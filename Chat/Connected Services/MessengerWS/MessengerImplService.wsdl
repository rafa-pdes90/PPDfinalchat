      <output>
        <soap12:body use="literal" />
      </output>
    </operation>
  </binding>
  <service name="MessengerImplService">
    <port name="MessengerImplPort" binding="tns:MessengerImplPortBinding">
      <soap12:address location="http://localhost:9999/messenger" />
    </port>
  </service>
